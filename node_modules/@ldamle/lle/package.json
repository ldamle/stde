{
    "name": "@ldamle/lle",
    "access": "public",
    "version": "1.0.53",
    "description": "Library for drawning and modeling logical elements",
    "main": "dist/lle.js",
    "types": "dist/types/lle.d.ts",
    "scripts": {
        "test": "bin/testlib&& bin/jestrunner all",
        "test:awotl": "bin/jestrunner awotl",
        "test:testlib": "bin/testlib&&bin/jestrunner testlib",
        "test:element": "bin/jestrunner element",
        "test:elementgraph": "bin/jestrunner elementgraph",
        "test:connection": "bin/jestrunner connection",
        "test:general": "bin/jestrunner general",
        "test:server": "bin/jestrunner server",
        "build": "rm -r dist/ && webpack --config webpack.config.js && tsc -p ./tsconfigd.json",
        "dev": "tsc -p ./tsconfigd.json --watch",
        "prettier": "prettier --write ./src/**/*.ts ./bin/**/*.ts ./test/**/*.ts ./examples/**/*.ts ./docs/**/*.ts",
        "eslint": "eslint src/ test/",
        "eslint:fix": "eslint --fix src/ test/",
        "typedoc": "typedoc",
        "codestyle": "npm run prettier;npm run eslint:fix",
        "package": "npm run typedoc;npm run test;npm run codestyle;npm run build"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run codestyle"
        }
    },
    "author": "pluttan",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.11.1",
        "@types/babel__traverse": "7.0.6",
        "@types/d3": "^7.4.3",
        "@types/deasync": "^0.1.5",
        "@types/express": "^5.0.0",
        "@types/jest": "^24.9.1",
        "@types/katex": "^0.16.7",
        "@types/node": "^12.7.5",
        "@typescript-eslint/parser": "^8.8.0",
        "docsify-cli": "^4.3.0",
        "dts-bundle-generator": "^9.5.1",
        "dts-bundle-webpack": "^1.0.2",
        "eslint": "^9.11.1",
        "eslint-plugin-jsdoc": "^50.3.0",
        "eslint-plugin-react": "^7.37.0",
        "formidable": "^3.5.1",
        "glob": "^11.0.0",
        "globals": "^15.9.0",
        "husky": "^3.0.5",
        "jest": "^24.9.0",
        "jest-html-reporters": "^3.1.7",
        "minimatch": "^10.0.1",
        "nyc": "^17.1.0",
        "nyc-dark": "3.0.3",
        "prettier": "^1.18.2",
        "replace-in-file-webpack-plugin": "^1.0.6",
        "string-replace-loader": "^3.1.0",
        "ts-jest": "^24.3.0",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.8.1",
        "tslint": "^5.20.0",
        "tslint-config-prettier": "^1.18.0",
        "typedoc": "^0.26.7",
        "typedoc-material-theme": "1.1.0",
        "typedoc-plugin-markdown": "^4.2.8",
        "typescript": "5.5.x",
        "typescript-eslint": "^8.7.0",
        "webpack-cli": "^5.1.4"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
